generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String @id @default(uuid())
  cognito_id String @unique
  handle     String @unique
  // dialogues   Dialogue[]

  @@map("users")
}
